%table.condensed-table
  %thead
    %tr
      %th= t ".id"
      %th= t ".status"
      %th= t ".tags"
  %tbody
    - entities.each do |entity|
      %tr
        %td= link_to entity.osm_id, project_node_path(entity.project, entity)
        %td
          = status_label(entity)
        %td= entity.tags_string
